print('===========Dictionary===================')
m_dictionary = {'John':'0989992211', 'Smith':'0892223333'}


'John' in m_dictionary # True  of False

if 'John' in m_dictionary : 
    print ('Found')

copy_values = m_dictionary.values()
print(copy_values)
print(type(copy_values))

copy_keys = m_dictionary.keys()
print(copy_keys)
print(type(copy_keys))

if 'John' in m_dictionary  and 'Smith' in m_dictionary: 
    print ('Found Both')

if 'x' not in m_dictionary : 
    print ('not found x')

